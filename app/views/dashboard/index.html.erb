<div class="dashboard">
  <!-- Nav tabs -->
  <ul class="nav nav-tabs" role="tablist">
    <li role="presentation" class="active"><a href="#created" aria-controls="created" role="tab" data-toggle="tab">Created</a></li>
    <li role="presentation"><a href="#participated" aria-controls="participated" role="tab" data-toggle="tab">Participated</a></li>
  </ul>

  <!-- Tab panes -->
  <div class="tab-content">
    <div role="tabpanel" class="tab-pane active" id="created">
      <div class="panel panel-default">
        <div class="panel-body">
          <ul>
            <% @created_rooms.each do |room| %>
              <li><%= room.name %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
    <div role="tabpanel" class="tab-pane" id="participated">
      <div class="panel panel-default">
        <div class="panel-body">
          <ul>
            <% @partipated_rooms.each do |room| %>
              <li><%= room.name %></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>

</div>